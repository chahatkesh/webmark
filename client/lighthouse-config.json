{
  "extends": "lighthouse:default",
  "settings": {
    "onlyCategories": ["seo", "best-practices", "accessibility"],
    "emulatedFormFactor": "desktop",
    "output": ["html", "json"],
    "maxWaitForLoad": 30000,
    "throttlingMethod": "simulate",
    "screenEmulation": {
      "mobile": false,
      "width": 1350,
      "height": 940,
      "deviceScaleFactor": 1,
      "disabled": false
    }
  }
}
