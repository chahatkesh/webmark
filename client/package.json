{
  "name": "client",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm install --include=dev && node --experimental-modules src/utils/sitemapGenerator.js && ./node_modules/.bin/vite build",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "generate-sitemap": "node --experimental-modules src/utils/sitemapGenerator.js",
    "update-sitemaps": "node --experimental-modules src/utils/sitemapGenerator.js && echo 'All sitemaps updated with current date'",
    "update-meta": "node --experimental-modules src/utils/metaDescriptionGenerator.js",
    "seo-analyze": "npx lighthouse https://webmark.site --config-path=./lighthouse-config.json --output-path=./seo-reports/lighthouse-report --view",
    "seo-validate": "npx schema-validator https://webmark.site",
    "seo-monitor": "node --experimental-modules src/utils/seoMonitor.js",
    "seo-full-update": "npm run update-meta && npm run generate-sitemap && npm run seo-analyze"
  },
  "dependencies": {
    "@radix-ui/react-checkbox": "^1.3.1",
    "@radix-ui/react-dialog": "^1.1.2",
    "@radix-ui/react-dropdown-menu": "^2.1.2",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-label": "^2.1.6",
    "@radix-ui/react-select": "^2.1.2",
    "@radix-ui/react-separator": "^1.1.6",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-tabs": "^1.1.1",
    "@tanstack/react-query": "^5.59.16",
    "axios": "^1.7.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "emoji-picker-react": "^4.12.0",
    "framer-motion": "^11.11.11",
    "lucide-react": "^0.453.0",
    "react": "^18.3.1",
    "react-beautiful-dnd": "^13.1.1",
    "react-dom": "^18.3.1",
    "react-helmet": "^6.1.0",
    "react-helmet-async": "^2.0.5",
    "react-icons": "^5.5.0",
    "react-router-dom": "^6.24.1",
    "react-toastify": "^10.0.6",
    "recharts": "^2.13.0",
    "tailwind-merge": "^2.5.4",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@types/node": "^22.8.1",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.14",
    "vite": "^5.3.1"
  }
}
